{
  "version": 2,
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10", "buildCommand":"bash setup.sh" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    },
        {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}